<query version="1.3">
  <block type="PRAGMA"/>
  <block type="FOR">
    <node variable="$order" port="Orders" window="unbounded"/>
    <node variable="$inventory" port="Inventory" window="attribute" keep="1" path="/Inventory_ns1:Event/Inventory_ns1:itemId"/>
  </block>
  <block type="LET"/>
  <block type="WHERE">
    <node path="$order/itemId" operator="EQ" value="$inventory/Inventory_ns1:itemId" cast="none" logical="AND"/>
    <node path="$order/quantity" operator="GT" value="$inventory/Inventory_ns1:quantity" cast="number" logical="AND"/>
  </block>
  <block type="ORDER-BY"/>
  <block type="GROUP-BY"/>
  <block type="RETURN">
    <node name="OutOfStock" type="element">
      <node name="itemId" type="element" value="$order/itemId" cast="string"/>
      <node name="shortage" type="element" value="$order/quantity - $inventory/Inventory_ns1:quantity" cast="string"/>
    </node>
  </block>
  <block type="OUTPUT">
    <node option="changes" every="false" interval="10" unit="second"/>
  </block>
</query>
]CdAtAeNd]></source><execute><![CDATA[
declare namespace Inventory_ns1 = "http://schema.vitria.com/Event/Types/InventoryEvent";
declare option outputstream "{out}";
declare output (1 as dom:element*);
(: End of Generated Header :)
for $order in qs:stream('{Orders}')
for $inventory in qs:window('{Inventory}', 'keep 1 events per /Inventory_ns1:Event/Inventory_ns1:itemId')

where $order/itemId = $inventory/Inventory_ns1:itemId
  and fn:number($order/quantity) > fn:number($inventory/Inventory_ns1:quantity)

return
    <OutOfStock>
        <itemId>{fn:string($order/itemId)}</itemId>
        <shortage>{fn:string($order/quantity - $inventory/Inventory_ns1:quantity)}</shortage>
    </OutOfStock>

output
    changes

]CdAtAeNd]></execute><sample><![CDATA[
<OutOfStock>
  <itemId __vitria__type="string"/>
  <shortage __vitria__type="string"/>
</OutOfStock>
]CdAtAeNd]></sample></embedded><param name="$dynamic-input-ports" value="Orders:Orders,Inventory:Inventory"/><param name="enableTSC" value="false"/><param name="tscInterval" value="5 minute"/><param name="tscRange" value="0 minute"/></widget></widgets><connection><link event="undefined"><source uuid="3B7EA30A-EBC9-0358-37F1-D704B179A607" port="source"/><target uuid="1C9BE341-7654-35E3-D5AB-D704C697AF26" port="@Orders"/></link><link event="undefined"><source uuid="1C9BE341-7654-35E3-D5AB-D704C697AF26" port="out"/><target uuid="3AE9D10D-3687-610D-CD35-D744409587F7" port="@Orders"/></link><link event="undefined"><source uuid="1C9BE341-7654-35E3-D5AB-D704C697AF26" port="out"/><target uuid="063F988F-4F2F-E2FF-3DE1-D77CB927DD57" port="@Orders"/></link><link event="undefined"><source uuid="1C9BE341-7654-35E3-D5AB-D704C697AF26" port="out"/><target uuid="2F8F7392-1203-FEF2-46D5-DC1118CE53FC" port="@Orders"/></link><link event="undefined"><source uuid="2F8F7392-1203-FEF2-46D5-DC1118CE53FC" port="out"/><target uuid="48D786C8-840C-685A-0A9F-DC111E6B2582" port="@Orders"/></link><link event="undefined"><source uuid="A11D4A92-704B-E6A1-0AB6-DC0F85017675" port="source"/><target uuid="48D786C8-840C-685A-0A9F-DC111E6B2582" port="@Inventory"/></link></connection></widget>]]></Data>
