<query version="1.3">
  <block type="PRAGMA"/>
  <block type="FOR">
    <node variable="$order" port="Orders" window="unbounded"/>
  </block>
  <block type="LET">
    <node uuid="5A863C2C-17D0-BFB1-2B50-DC13D53B0117" variable="$item" block="FOR-AT">
      <block type="FOR-AT">
        <node variable="$fv1" index="$i" path="$order/Orders_ns1:order/Orders_ns2:order/items"/>
      </block>
      <block type="LET"/>
      <block type="WHERE"/>
      <block type="GROUP-BY"/>
      <block type="ORDER-BY"/>
      <block type="RETURN">
        <node name="item" type="element" cast="string">
          <node name="itemId" type="element" value="$fv1/itemId" cast="string"/>
          <node name="quantity" type="element" value="$fv1/quantity" cast="string"/>
        </node>
      </block>
    </node>
  </block>
  <block type="WHERE"/>
  <block type="ORDER-BY"/>
  <block type="GROUP-BY"/>
  <block type="RETURN">
