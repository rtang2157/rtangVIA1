  <query saveContext="false" title="GetOrderItems" name="2F8F7392-1203-FEF2-46D5-DC1118CE53FC">
    <xquery><![CDATA[
declare namespace Orders_ns2 = "http://tutorials.schema.vitria.com/order";
declare namespace Orders_ns1 = "http://schema.vitria.com/Event/Types/OrderEvent";
declare option qs:checkpoint "off";
declare option outputstream "2F8F7392-1203-FEF2-46D5-DC1118CE53FC-Out";
