declare namespace Inventory_ns1 = "http://schema.vitria.com/Event/Types/InventoryEvent";
declare option qs:checkpoint "off";
declare option outputstream "48D786C8-840C-685A-0A9F-DC111E6B2582-Out";
declare output (1 as dom:element*);
(: End of Generated Header :)
for $order in qs:stream('2F8F7392-1203-FEF2-46D5-DC1118CE53FC-Out')
