declare namespace Orders_ns2 = "http://tutorials.schema.vitria.com/order";
declare namespace Orders_ns1 = "http://schema.vitria.com/Event/Types/OrderEvent";
declare option outputstream "{out}";
declare output (1 as dom:element*);
(: End of Generated Header :)
for $order in qs:stream('{Orders}')

let $item := {
                 for $fv1 at $i in $order/Orders_ns1:order/Orders_ns2:order/items
                 return
                     <item>
                         <itemId>{fn:string($fv1/itemId)}</itemId>
                         <quantity>{fn:string($fv1/quantity)}</quantity>
                     </item>
             }

return
    {$item}

output
    changes

]CdAtAeNd]></execute><sample><![CDATA[
