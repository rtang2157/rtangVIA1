    <OutOfStock>
        <itemId>{fn:string($order/itemId)}</itemId>
        <shortage>{fn:string($order/quantity - $inventory/Inventory_ns1:quantity)}</shortage>
    </OutOfStock>

output
    changes
]CdAtAeNd]></xquery>
